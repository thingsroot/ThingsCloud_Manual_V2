{
    "gitbook": "3.x.x",
    "plugins": ["favicon", "yahei", "autotheme", "-lunr", "-search",
        "search-plus", "prism", "-highlight",  "splitter",
        "-sharing", "sharing-plus", "tbfed-pagefooter", 
        "book-summary-scroll-position-saver", "back-to-top-button",
        "copy-code-button", "multipart"
    ],
    "pluginsConfig": {
        "theme-default": {
            "showLevel": true
        },
        "fontSettings": {
            "family": "serif",
            "size": 2
        },

        "prism": {
            "css": [
                "prismjs/themes/prism-tomorrow.css"
            ]
        },
    "pdf": {
        "pageNumbers": true,
        "fontFamily": "Arial",
        "fontSize": 12,
        "paperSize": "a4",
        "margin": {
            "right": 62,
            "left": 62,
            "top": 56,
            "bottom": 56
        }
    },
    "title": "冬笋云-参考文档",
    "description": "冬笋云-参考文档",
    "structure": {
        "readme": "README.md"
    }
    }
}